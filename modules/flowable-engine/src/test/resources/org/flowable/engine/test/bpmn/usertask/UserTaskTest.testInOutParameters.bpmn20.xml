<?xml version="1.0" encoding="UTF-8"?>
<definitions
        xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
        xmlns:flowable="http://flowable.org/bpmn"
        targetNamespace="Examples">

  <process id="userTaskInOutProcess">
    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="task" />
    <userTask id="task" flowable:assignee="testUser">
      <extensionElements>
        <flowable:in source="processVariable1" target="taskVariable1"></flowable:in>
        <flowable:in sourceExpression="${processVariable2}" target="taskVariable2"></flowable:in>
        <flowable:out source="test" target="test"></flowable:out>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="task" targetRef="theEnd" />
    <endEvent id="theEnd" />
  </process>

</definitions>
